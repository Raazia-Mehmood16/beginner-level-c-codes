#include<iostream>
using namespace std;

int main()
{
	int rows = 0, columns = 0, t = 0;
	int arr[100][100];
	cout << "Enter number of rows: ";
	cin >> rows;
	cout << "Enter number of columns: ";
	cin >> columns;
	cout << "Enter Target Value: ";
	cin >> t;
	cout << "Enter Your Elements: ";

	for (int i = 0; i < rows; i++)
	{
		for (int j = 0; j < columns; j++)
		{
			cin >> arr[i][j];
		}
	}

	for (int i = 0; i < rows; i++)
	{
for (int j = 0; j < columns; j++)
		{   
			if (arr[i][j + 1] < t)
			{
				j += 1;
			}
			else if (arr[i + 1][j] < t)
			{
				i += 1;
			}
			else if(arr[i][j+1]==t||arr[i+1][j]==t)
			{
				cout << "Path Found ";
				return 0;
			}
			else
			{
				cout << "No Path Found ";
				return 0;
			}
		}
	}
	cout << "No Path Found ";
	return 0;
	
}