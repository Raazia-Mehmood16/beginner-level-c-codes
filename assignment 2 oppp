#include<iostream>
using namespace std;

int main()
{
    int size;
    cout << "Enter Size of Array : ";
    cin >> size;
    int *arry =new int [size];
    cout << "Enter Array "<<size<<" Elements : " << endl;
    int count = 0;
    for (int i=0;i < size;++i)
    {
        cin >> arry[i];
        count++;
    }
    if (count > size)
    {
        int *temp = new int[size * 2];
        for (int j = 0;j < size * 2;++j)
        {
            temp[j] = arry[j];
            arry = temp;
            delete[] arry;
        }
        cout << "As You Entered more number from given size so : " << endl;
        cout << "New Array is : " << endl;
        for (int j = 0;j < size * 2;++j)
        {
            cout << temp[j] << " ";
        }
    }
    else
    {
        cout << "perivous array is : " << endl;
        for (int j = 0;j < size;++j)
        {
            cout <<arry[j] << " ";
        }
    }
    

    return 0;
}